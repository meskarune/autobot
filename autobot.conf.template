[irc]
network = irc.freenode.net
port = 7000
channels = ##test, ##test2
nick = autobot
nickpass = password
name = Bot
ssl = True

[tcp]
host: localhost
port: 47998

[bot]
prefix = !
log_scheme = ./logs/{channel}/%%Y-%%m-{channel}.log

[plugins]
commands = hello, goodbye, slap, ugt, rot13, throw
OP_commands = die, disconect
passive = url_announce, LogFile
